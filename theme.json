{
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "620px",
            "wideSize": "1000px"
        },
        "spacing": {
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "System Font",
                    "slug": "system-font"
                },
                {
                    "name": "NanumGothic",
                    "slug": "nanumgothic",
                    "fontFamily": "NanumGothic",
                    "fontFace": [
                        {
                            "fontFamily": "NanumGothic",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/fonts/nanumgothic/nanumgothic-400-normal.ttf"
                            ]
                        },
                        {
                            "fontFamily": "NanumGothic",
                            "fontStyle": "normal",
                            "fontWeight": "600",
                            "src": [
                                "file:./assets/fonts/nanumgothic/nanumgothic-600-normal.ttf"
                            ]
                        },
                        {
                            "fontFamily": "NanumGothic",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/fonts/nanumgothic/nanumgothic-700-normal.ttf"
                            ]
                        }
                    ]
                }
            ]
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "color": {
            "gradient": "var(--wp--preset--gradient--very-light-gray-to-cyan-bluish-gray)",
            "text": "var(--wp--preset--color--black)"
        },
        "elements": {
            "button": {
                "color": {
                    "gradient": "var(--wp--preset--gradient--blush-light-purple)",
                    "text": "var(--wp--preset--color--black)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--nanumgothic)"
                }
            },
            "caption": {
                "color": {
                    "text": "var(--wp--preset--color--vivid-cyan-blue)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--nanumgothic)"
                }
            },
            "heading": {
                "color": {
                    "gradient": "var(--wp--preset--gradient--vivid-cyan-blue-to-vivid-purple)",
                    "text": "var(--wp--preset--color--white)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--nanumgothic)"
                }
            },
            "link": {
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--pale-cyan-blue)"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--vivid-purple)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--nanumgothic)"
                }
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--nanumgothic)",
            "fontSize": "var(--wp--preset--font-size--small)"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header"
        },
        {
            "area": "footer",
            "name": "footer"
        }
    ],
    "version": 3,
    "$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}